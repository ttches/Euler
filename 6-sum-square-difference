
function sumOfSquares(n) {
    var sum = 0;
    for (var i = 1; i <= n; i++) {
        sum += i*i;
    }
    return sum;
}

function squareOfSums(n) {
    var sum = 0;
    for (var i=1; i <= n; i++) {
        sum += i;
    }
    sum *= sum;
    return sum;
}

function funcDif(n) {
    return squareOfSums(n) - sumOfSquares(n);
}

funcDif(100);
